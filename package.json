{
  "name": "react-giphy",
  "version": "1.0.0",
  "description": "Giphy Search with ReactJS",
  "main": "main.js",
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.dev.babel.js --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --config config/webpack.prod.babel.js --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.babel.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "start": "npm run server",
    "deploy": "git subtree push --prefix dist origin gh-pages",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://www.github.com/stepquick/react-giphy"
  },
  "keywords": [
    "reactjs",
    "giphy"
  ],
  "author": "Stephen Quick",
  "private": true,
  "dependencies": {
    "milligram": "^1.3.0",
    "normalize.css": "^5.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.18",
    "babel-runtime": "^6.20",
    "clean-webpack-plugin": "^0.1.9",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "open-browser-webpack-plugin": "0.0.3",
    "react-hot-loader": "^1.3.1",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^2.4.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  }
}
